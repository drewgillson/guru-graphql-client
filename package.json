{
  "name": "transcriber",
  "homepage": "https://llooker.github.io/transcriber/",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-client-preset": "^1.0.2",
    "apollo-link-http": "^1.5.16",
    "gh-pages": "^2.1.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.10.1",
    "local-storage": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "reset-css": "^2.2.1",
    "styled-components": "^2.2.3"
  },
  "scripts": {
    "start": "env-cmd ../../config/security.env.local react-scripts start",
    "build": "env-cmd ../../config/security.env.prod react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy-s3": "yarn build && serverless client deploy",
    "deploy-gh": "gh-pages -d build",
    "deploy-netlify": "yarn build && netlify deploy build"
  },
  "devDependencies": {
    "env-cmd": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "react-test-renderer": "^16.12.0",
    "serverless-finch": "^1.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
